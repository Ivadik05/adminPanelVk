{
  "name": "admin-vk",
  "version": "0.0.1",
  "description": "Admin Panel Vk",
  "main": "index.js",
  "scripts": {
    "cpsettings": "cp ./src/settings/settings.js ./public/dist",

    "build-client:dev": "BUILD_DEV=1 ./node_modules/webpack/bin/webpack.js",
    "build-server:dev": "BUILD_DEV=1 ./node_modules/webpack/bin/webpack.js --config webpack.config.server.js",
    "build:dev": "npm run build-client:dev && npm run build-server:dev",

    "build:dev-watch": "BUILD_DEV=1 ./node_modules/webpack/bin/webpack.js --watch",
    "build:prod": "./node_modules/webpack/bin/webpack.js -p --define process.env.NODE_ENV='\"production\"'",

    "dev-server": "BUILD_DEV=1 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --hot --inline --content-base public",
    "typings:install": "./node_modules/typings/dist/typings.js install",
    "start": "node dist/server.js",
    "build-start:dev": "npm run build:dev && npm run start",

    "test": "./node_modules/jest-cli/bin/jest.js"
  },
  "files": [
    "public/",
    "install.sh"
  ],
  "author": "Vadim.ch",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "husky": "^0.10.2",
    "jest-cli": "0.9.2",
    "js-md5": "^0.4.0",
    "js-sha256": "^0.3.0",
    "moment": "^2.11.2",
    "node-jsx": "^0.13.3",
    "object-assign": "^4.0.1",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-shallow-compare": "^0.14.7",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.0.6",
    "redux-logger": "^2.4.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "tsd": "^0.6.5",
    "tslint": "^3.6.0",
    "tslint-loader": "^2.1.0",
    "typescript": "1.8.2",
    "typings": "^0.7.12",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "react-document-title": "^2.0.1"
  }
}
